{
  "expo": {
    "name": "College Study",
    "slug": "hbtu-college-study",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appicon.jpg",
    "userInterfaceStyle": "automatic",
    "developmentClient": {
      "silentLaunch": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff",
      "barStyle": "dark-content"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.collegestudy",
      "infoPlist": {
        "UIBackgroundModes": [
          "background-fetch",
          "background-processing",
          "background-fetch",
          "background-processing"
        ],
        "NSCameraUsageDescription": "This app uses the camera to upload profile pictures and documents.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to upload images.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "icon": "./assets/appicon.jpg",
      "package": "com.collegestudy",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "WAKE_LOCK",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/appicon.jpg",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow College Study to access media so downloads can appear in your library.",
          "savePhotosPermission": "Allow College Study to save downloaded notes to your device.",
          "isAccessMediaLocationEnabled": false
        }
      ]
    ],
    "scheme": "hbtu-study",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "130a9f9e-4605-4341-a26c-100c228a47a2"
      }
    },
    "experiments": {
      "typedRoutes": false
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
